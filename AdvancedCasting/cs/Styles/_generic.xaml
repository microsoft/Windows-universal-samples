                                 Foreground="{TemplateBinding CalendarItemForeground}"
                                 Style="{StaticResource WeekDayNameStyle}"/>
                      <TextBlock Grid.Column="2"
                                 Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=TemplateSettings.WeekDay3}"
